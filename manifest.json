{
  "name": "Sticky for goo.glâ„¢",
  "short_name": "Sticky",
  "description": "Make goo.gl a little bit more useful by adding labels and tags.",
  "version": "0.1",
  "icons": {
    "16": "16.png",
    "48": "48.png",
    "128": "128.png"
  },
  "permissions": [
    "https://*.goo.gl/*",
    "tabs"
  ],
  "content_scripts": [{
    "matches": [
      "https://*.goo.gl/*"
    ],
    "css": [
      "./lib/taggle.css"
    ],
    "js": [
      "./lib/taggle.js",
      "./sticky.js"
    ],
    "run_at": "document_start"
  }],
  "manifest_version": 2
}
